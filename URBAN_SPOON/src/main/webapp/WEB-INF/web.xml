<?xml version="1.0" encoding="UTF-8"?>
<web-app>
  <display-name>URBAN_SPOON</display-name>
  <servlet>
  <servlet-name>login</servlet-name>
  <servlet-class>loginCheck</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>login</servlet-name>
  <url-pattern>/log</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>SignUp</servlet-name>
  <servlet-class>signCheck</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>SignUp</servlet-name>
  <url-pattern>/signUp</url-pattern>
  </servlet-mapping>
  
  
  
  <servlet>
  <servlet-name>reservation</servlet-name>
  <servlet-class>reservationCheck</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>reservation</servlet-name>
  <url-pattern>/reservation</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
  <servlet-name>reviews</servlet-name>
  <servlet-class>reviewscheck</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>reviews</servlet-name>
  <url-pattern>/reviews</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>getreviews</servlet-name>
  <servlet-class>getreviews</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>getreviews</servlet-name>
  <url-pattern>/getreviews</url-pattern>
  </servlet-mapping>
  
  
  
  
  <servlet>
  <servlet-name>forgotPassword</servlet-name>
  <servlet-class>forgotCheck</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>forgotPassword</servlet-name>
  <url-pattern>/forgotPassword</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
  <welcome-file>mainpage.html</welcome-file>   
  </welcome-file-list>
  
</web-app>